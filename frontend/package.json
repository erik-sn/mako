{
  "name": "mako-frontend",
  "version": "0.0.4",
  "description": "Mako frontend",
  "main": "index.tsx",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts*'",
    "test": "jest --no-cache",
    "coverage": "jest --color --coverage",
    "dev": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js --mode development",
    "build": "./node_modules/.bin/webpack --config webpack.prod.js --mode production",
    "compile": "tsc --outdir dist -t es5 -m commonjs",
    "server": "node ./dist/src/server.js"
  },
  "author": "Erik Niehaus",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/__tests__/setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/fileMock.ts",
      "\\.(css|scss|less)$": "<rootDir>/__tests__/styleMock.ts",
      "@(.*)$": "<rootDir>/src$1"
    },
    "testPathIgnorePatterns": [
      "setup.ts",
      "fileMock.ts",
      "styleMock.ts",
      "dist"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "_moduleAliases": {
    "@": "dist/src"
  },
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/classnames": "^2.2.3",
    "@types/compression": "0.0.35",
    "@types/enzyme": "^3.1.7",
    "@types/express": "^4.11.0",
    "@types/express-handlebars": "0.0.30",
    "@types/jest": "^22.0.1",
    "@types/jsdom": "^11.0.4",
    "@types/luxon": "^0.2.6",
    "@types/material-ui": "^0.21.3",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-dropzone": "^4.2.0",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-transition-group": "^2.0.7",
    "@types/sinon": "^4.1.3",
    "autoprefixer": "8.1.0",
    "axios": "0.18.0",
    "babel-loader": "^7.1.4",
    "chai": "^4.1.2",
    "css-loader": "^0.28.8",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^21.2.1",
    "jsdom": "^11.5.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.2.0",
    "module-alias": "^2.0.3",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.10",
    "prop-types": "^15.6.0",
    "react-hot-loader": "^4.0.0",
    "react-overdrive": "^0.0.10",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^3.3.0",
    "style-loader": "^0.18.2",
    "tapable": "1.0.0-beta.5",
    "ts-jest": "^22.0.1",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.1.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "classnames": "^2.2.5",
    "core-js": "^2.5.1",
    "debounce": "^1.1.0",
    "formik": "^0.11.11",
    "is-url": "^1.2.2",
    "luxon": "^0.5.3",
    "material-ui-icons": "^1.0.0-beta.36",
    "morgan": "^1.9.0",
    "raf": "^3.3.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-dropzone": "^4.2.10",
    "react-infinite": "^0.12.1",
    "react-masonry-infinite": "^1.2.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "yup": "^0.24.1"
  }
}
